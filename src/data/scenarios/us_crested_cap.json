        "on_activate_expr": "set_global(\"is_tool_tut_finished\", false);\n\nsleep_ui(1.0);\ntutorial_dialog(\"Welcome to Operational Command. This mission will serve to familiarize you with the tools at your disposal.\", \"%Map\", true);\nsleep_ui(1.0);\ntutorial_dialog(\"Let's start with the most important; This is your map. It shows you the current terrain.\", \"%Map\", true);\nsleep_ui(1.0);\ntutorial_dialog(\"This is the briefing document. Here you can see the Mission Operations Order and most importantly; your objectives (LMB + drag to move it. 2x LMB to view. RMB to stop).\", \"%BriefingDoc\", true);\nsleep_ui(1.0);\ntutorial_dialog(\"This is the Radio Transcript. Here you will find a transcript of all radio messages since the start of the mission.\", \"%TranscriptDoc\", true);\nsleep_ui(1.0);\ntutorial_dialog(\"This is the intelligence document. Here you will find mission specific intel like enemy disposition.\", \"%IntelDoc\", true);\nsleep_ui(1.0);\ntutorial_dialog(\"This is a protractor. It can be used to gague more accurate distances and compass headings.\", \"%Protractor\", true);\nsleep_ui(1.0);\ntutorial_dialog(\"This is your Field Manual. In here you can find everything you'll learn in this tutorial and everything else.\", \"%FieldManual\", true);\nsleep_ui(1.0);\ntutorial_dialog(\"This is your radio. To transmitt you need to hold down the PTT (SPACE). Try saying \\\"Radio Check\\\".\", \"%Radio\", true);\nset_global(\"is_tool_tut_finished\", true);",
